      "cvrf:Note": [
        {
          "#text": "An exposure of sensitive information to an unauthorized actor vulnerability in FortiOS CLI may allow a local and authenticated user assigned to a specific VDOM to retrieve other VDOMs information such as the admin account list and the network interface list.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
