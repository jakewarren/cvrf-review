    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "The FortiWLC file management AP script download webUI page is affected by an OS Command Injection vulnerability which may allow an authenticated admin user to execute arbitrary system console commands, and possibly subsequently \"root\" the device.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiWLC 6.1-2 -> 6.1-5FortiWLC 7.0-7 -> 7.0-10FortiWLC 8.0 -> 8.2FortiWLC 8.3.0 -> 8.3.2",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "For FortiWLC 7.x branch, upgrade to 7.0.11 or newer versions. For FortiWLC 8.x branch, upgrade to 8.3.3 or newer versions.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
