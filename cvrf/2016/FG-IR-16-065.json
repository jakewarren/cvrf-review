    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "FortiWLC comes with a hardcoded account named 'core' which is used by Meru Access Points to send core dumps to the FortiWLC and has read/write privileges over various parts of the system.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Unauthorized read/write remote access",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiWLC 7.0-9-1, 7.0-10-0, 8.1-2-0, 8.1-3-2 and 8.2-4-0",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Depending on your version, apply the following patches:7.0-9-1:meru-7.0-9-1-patch-bug03932927.0-10-0:meru-7.0-10-0-patch-bug03932928.1-2-0:meru-8.1-2-0-patch-bug03932928.1-3-2:meru-8.1-2-0-patch-bug03932928.2-4-0:meru-8.2-4-0-patch-bug0393292",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
