    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An improper neutralization of special elements used in an OS command vulnerability [CWE-78] in the management interface of FortiADC may allow an authenticated attacker to execute unauthorized commands via specifically crafted arguments to existing commands.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "None",
          "@Ordinal": "2",
          "@Title": "Description",
          "@Type": "General"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "3",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiADC 7.4 all versions are not affectedFortiADC 7.2 all versions are not affectedFortiADC version 7.1.0 through 7.1.1FortiADC version 7.0.0 through 7.0.3FortiADC version 6.2.0 through 6.2.5FortiADC 6.1 all versionsFortiADC 6.0 all versions are not affectedFortiADC 5.4 all versions are not affectedFortiADC 5.3 all versions are not affected",
          "@Ordinal": "4",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Please upgrade to FortiADC version 7.1.2 or abovePlease upgrade to FortiADC version 7.0.4 or abovePlease upgrade to FortiADC version 6.2.6 or above",
