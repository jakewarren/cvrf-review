    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An improper neutralization of special elements used in an SQL Command ('SQL Injection') vulnerability [CWE-89] in FortiSandbox may allow a remote and authenticated attacker with read permission to retrieve arbitrary files from the underlying Linux system via a crafted HTTP request.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "None",
          "@Ordinal": "2",
          "@Title": "Description",
          "@Type": "General"
        },
        {
          "#text": "Information disclosure",
          "@Ordinal": "3",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "At leastFortiSandbox version 4.2.0FortiSandbox version 4.0.0 through 4.0.2FortiSandbox version 3.2.0 through 3.2.3FortiSandbox version 3.0.1 through 3.0.7FortiSandbox 3.1 all versions",
          "@Ordinal": "4",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Please upgrade to FortiSandbox version 4.2.1 or above Please upgrade to FortiSandbox version 4.0.3 or above Please upgrade to FortiSandbox version 3.2.4 or above",
          "@Ordinal": "5",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
