    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An improper input validation (CWE-20) vulnerability in FortiAP CLI admin console may allow unauthorized administrators to overwrite system files via specially crafted tcpdump commands in the CLI.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiAP-S/W2 6.2.0 to 6.2.2, 6.0.5 and belowFortiAP-U 6.0.1 and belowFortiAP is not impactedFortiAP-C is not impacted",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Upgrade to FortiAP-S/W2 6.0.6 or 6.2.3 and above Upgrade to FortiAP-U 6.0.2 or above Revision History: 02-10-2020 Initial version 05-25-2020 Added FAP, FAP-U and FAP-C impact info.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
