    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An improper neutralization of input during web page generation vulnerability [CWE-79] in FortiOS may allow a remote unauthenticated attacker to either redirect users to malicious websites via a crafted \"Host\" header or to execute JavaScript code in the victim's browser context.This happens when the FortiGate has web filtering and category override enabled/configured.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiOS version 6.4.1 and below.FortiOS version 6.2.9 and below.",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Please upgrade to FortiOS version 6.4.2 or above.Please upgrade to FortiOS version 6.2.10 or above.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
