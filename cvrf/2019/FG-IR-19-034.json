    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "Failure to sanitize input in the SSL VPN web portal may allow an attacker to perform a reflected Cross-site Scripting (XSS) attack via multiple parameters of the error page HTTP request.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "CVE-2019-5586FortiOS 6.0.0 to 6.0.4FortiOS 5.2.0 to 5.6.10CVE-2019-5588FortiOS 6.0.0 to 6.0.4",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Upgrade to FortiOS 5.6.11, 6.0.5 or 6.2.0 Workarounds: Disable the SSL-VPN web portal service by applying the following CLI commands: config vpn ssl settings unset source-interface end Revision History: 05-24-2019 Initial version 08-21-2019 Add 5.6 branch fixing for CVE-2019-5586",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
