    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "The default replacement message in FortiOS' Application Control block page reveals the private IP as well as the hostname of the FortiGate.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Information disclosure",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiOS 5.6.5, 6.0.1 and below.",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Upgrade to 5.6.6, 6.0.2, 6.2.0 or laterWork around:All the replacement messages are configurable by the administrators. The default replacement messages are just templates. An administrator can easily change them to suit their needs. For example, remove the server/client IPs and FortiOS host names.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
