          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiWeb versions 6.4.1 and below.FortiWeb versions 6.3.15 and below.FortiWeb versions 6.2.6 and below.FortiWeb versions 6.1.2 and below.FortiWeb versions 6.3.15 and below.FortiWeb versions 6.0.7 and below.FortiADC versions 7.0.2 and below.FortiADC versions 6.2.4 and below.FortiADC 6.1, 6.0, 5.4, 5.3, 5.2,5.1,5.0 all versions",
