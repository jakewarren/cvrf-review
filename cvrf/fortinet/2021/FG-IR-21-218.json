    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An incomplete filtering of one or more instances of special elements vulnerability [CWE-792] in the command line interpreter of FortiRecorder and FortiWeb may allow an authenticated user to read arbitrary files via specially crafted command arguments.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "None",
          "@Ordinal": "2",
          "@Title": "Description",
          "@Type": "General"
        },
        {
          "#text": "Information disclosure",
          "@Ordinal": "3",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiWeb version 6.4.0 through 6.4.1FortiWeb version 6.3.0 through 6.3.17FortiWeb all versions 6.2FortiWeb all versions 6.1FortiWeb all versions 6.0FortiRecorder version 6.4.0 through 6.4.3FortiRecorder all versions 6.0FortiRecorder all versions 2.7",
          "@Ordinal": "4",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Upgrade to FortiWeb version 7.0.0 or above,Upgrade to FortiWeb version 6.4.2 or above.Upgrade to FortiWeb version 6.3.18 or above.Upgrade to FortiRecorder version 7.0.0 or aboveUpgrade to FortiRecorder version 6.4.4 or above",
          "@Ordinal": "5",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
