    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "A stack-based buffer overflow vulnerability in the HTTPD daemon of FortiProxy may allow an authenticated remote attacker to crash the service by sending a malformed PUT request to the server.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiProxy versions 2.0.1 and below.FortiProxy versions 1.2.9 and below.FortiProxy versions 1.1.x.FortiProxy versions 1.0.x.",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Please upgrade to Fortiproxy version 1.2.10 or above.Please upgrade to Fortiproxy version 2.0.2 or above.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
