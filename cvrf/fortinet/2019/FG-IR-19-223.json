    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An improper neutralization of input during web page generation in the SSL VPN portal of FortiOS may allow a remote authenticated attacker to perform a stored cross site scripting attack (XSS).",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiOS version 6.2.1 and below. FortiOS version 6.0.8 and below. FortiOS version 5.6.12 and below.",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Please upgrade to FortiOS version 6.2.2 or above. Please upgrade to FortiOS version 6.0.9 or above. Please upgrade to FortiOS version 5.6.13 or above.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
