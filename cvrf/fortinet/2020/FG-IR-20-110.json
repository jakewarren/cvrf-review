      "cvrf:Note": [
        {
          "#text": "An execution with unnecessary privileges vulnerability in the VCM engine of FortiClient for Linux may allow local users to elevate their privileges to root by creating a malicious script or program on the target machine.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "None",
          "@Ordinal": "2",
          "@Title": "Description",
          "@Type": "General"
        },
        {
          "#text": "Escalation of privilege",
          "@Ordinal": "3",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiClient for Linux versions 6.2.7 and below. FortiClient for Linux versions 6.4.0 and below.",
          "@Ordinal": "4",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Please upgrade to FortiClient for Linux versions 6.2.8 or above. Please upgrade to FortiClient for Linux versions 6.4.1 or above.",
          "@Ordinal": "5",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
