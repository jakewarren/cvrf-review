      "cvrf:Note": [
        {
          "#text": "An improper neutralization of input vulnerability in FortiGate Cloud may allow a remote authenticated attacker to perform a stored cross site scripting attack (XSS) via the comments section of the Security Profiles.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiGate Cloud Version 20.3 and below.",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Fortinet has remediated this issue in FortiGate Cloud version 20.4 and hence the customers do not need to perform any action.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
