    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "A cleartext storage of sensitive information in the GUI of FortiADC, FortiSIEM, FortiDDoS, FortiDDoS-CM and FortiDDoS-F may allow a remote authenticated attacker to retrieve some sensitive information such as users LDAP passwords, RADIUS shared secret and the Elastic Cloud database password by deobfuscating the passwords entry fields.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Information disclosure",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiADC versions 6.0.0.FortiADC versions 5.x upto 5.4.3FortiSIEM versions 6.2.x, 6.1.x, 6.xFortiDDoS versions 5.4.x, 5.3.x, 5.2.x, 5.1.x, 5.0.xFortiDDoS-F versions 6.1.x, 6.0.x",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Please upgrade to FortiADC versions 5.4.4 or above.Please upgrade to FortiADC versions 6.0.1 or above.Please upgrade to FortiSIEM 6.3.0 or above.Please upgrade to FortiDDoS 5.5.0 or above.Please upgrade to FortiDDoS-F 6.2.0 or above.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
