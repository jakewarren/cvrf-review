      "Title": "XSS Vulnerability in FortiWeb Site Publisher",
      "cvrf:CVE": "CVE-2017-3129"
    },
    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "The Site Publisher functionality of FortiWeb has been found vulnerable to a Cross-Site Scripting vulnerability via an improperly sanitized parameter in a POST request.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiWeb versions below 5.7.1",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Upgrade to FortiWeb version 5.8.0",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
