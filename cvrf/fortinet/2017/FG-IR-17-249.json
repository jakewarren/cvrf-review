    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An old Infineon RSA library does not properly generate RSA key pairs, therefore enabling an attacker to potentially infer a private key from a public key.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Information disclosure",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "The following Fortinet products are NOT affected:FortiOSFortiSwitchFortiAPFortiAnalyzerFortiMailfortiManagerFortiWebFortiTokenFortiAuthenticatorNone of the products above are using the affected Infineon RSA library or TPM chips.",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        }
      ]
