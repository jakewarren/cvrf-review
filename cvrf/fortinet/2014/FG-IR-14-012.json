    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "FortiWeb 5.0, 5.1 and 5.2.0 are vulnerable to multiple reflective cross-site scripting issues. Several parameters in the web management interface URLs /user/ldap_user/check_dlg and /user/radius_user/check_dlg lack sufficient input filtering.",
          "@Ordinal": "1",
          "@Title": "Description",
          "@Type": "General"
        },
        {
          "#text": "Cross-Site Scripting",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiWeb 5.0.x, 5.1.x and 5.2.0.",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Upgrade to FortiWeb 5.2.1 or higher.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
