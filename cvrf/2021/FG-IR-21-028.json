          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "None",
          "@Ordinal": "2",
          "@Title": "Description",
          "@Type": "General"
        },
        {
          "#text": "Improper access control",
          "@Ordinal": "3",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiMail version 7.0.0 and below.FortiMail version 6.4.5 and below.FortiMail version 6.2.7 and below.FortiMail version 6.0.11 and below.FortiMail version 5.4.12 and below.",
          "@Ordinal": "4",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Upgrade to FortiMail version 7.0.1.Upgrade to FortiMail version 6.4.6.Upgrade to FortiMail version 6.2.8.Upgrade to FortiMail version 6.0.12.",
