    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An access of uninitialized pointer (CWE-824) vulnerability in FortiWLC may allow a local and authenticated attacker to crash the access point being managed by the controller by executing a crafted CLI command.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "At leastFortiWLC version 8.0.6FortiWLC version 8.1.2 through 8.1.3FortiWLC version 8.2.4 through 8.2.7FortiWLC version 8.3.0 through 8.3.3FortiWLC version 8.4.0 through 8.4.8FortiWLC version 8.5.0 through 8.5.5FortiWLC version 8.6.0 through 8.6.2",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Please upgrade to FortiWLC version 8.6.3 or above.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
