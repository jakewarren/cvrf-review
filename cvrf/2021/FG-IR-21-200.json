    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An improper access control vulnerability [CWE-284] in FortiWLC may allow an authenticated and remote attacker with low privileges to execute any command as an admin user with full access rights via bypassing the GUI restrictions.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "None",
          "@Ordinal": "2",
          "@Title": "Description",
          "@Type": "General"
        },
        {
          "#text": "Improper access control",
          "@Ordinal": "3",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiWLC versions 8.6.1 and below.FortiWLC versions 8.5.x.FortiWLC versions 8.4.x.FortiWLC versions 8.3.x.FortiWLC versions 8.2.x.FortiWLC versions 8.1.x.FortiWLC versions 8.0.x.",
          "@Ordinal": "4",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Please upgrade to FortiWLC version 8.6.2 or above.",
          "@Ordinal": "5",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
