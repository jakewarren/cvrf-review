    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "An instance of improper neutralization of special elements in FortiSandbox's sniffer module may allow an authenticated administrator to execute commands on the underlying system's shell via altering the content of its configuration file.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Execute unauthorized code or commands",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiSandbox 3.2.1 and below.FortiSandbox 3.1 all versionsFortiSandbox 3.0 all versions",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Upgrade to version 4.0.0 or above.Upgrade to version 3.2.2 or above.Upgrade to version 3.1.5 or above.Upgrade to version 3.0.7 or above.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
