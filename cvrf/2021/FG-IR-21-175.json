    "cvrf:DocumentNotes": {
      "cvrf:Note": [
        {
          "#text": "A plaintext storage of a password vulnerability [CWE-256] in the FortiSIEM Windows Agent may allow an authenticated user to impersonate the agent registered to the Supervisor via reading specific log files.",
          "@Ordinal": "1",
          "@Title": "Summary",
          "@Type": "Summary"
        },
        {
          "#text": "Information disclosure",
          "@Ordinal": "2",
          "@Title": "Impact",
          "@Type": "General"
        },
        {
          "#text": "FortiSIEM Windows Agent version 4.1.4 and below.FortiSIEM Windows Agent version 4.0.0.FortiSIEM Windows Agent version 3.3.0.FortiSIEM Windows Agent version 3.2.2 and below.FortiSIEM Windows Agent version 3.1.2 and below.",
          "@Ordinal": "3",
          "@Title": "Affected Products",
          "@Type": "General"
        },
        {
          "#text": "Upgrade to FortiSIEM Windows Agent version 4.1.5 or above.",
          "@Ordinal": "4",
          "@Title": "Solutions",
          "@Type": "General"
        }
      ]
    },
